<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fang.mapper.SongAndPlayListMapper">
    <!-- 根据id查询歌曲 -->
    <select id="selectByMusicId" resultType="com.fang.entity.dto.SongAndPlayListDTO">
        SELECT `id`,
               `play_list_id`,
               `song_music_id`
        FROM `song_and_play_list`
        WHERE `play_list_id` = #{id};
    </select>


</mapper>